	CALL	SUB1
	ST	GR7,#0050
	LD	GR3,=225
	LD	GR4,=30
	CALL	SUB1
	ST	GR7,#0051
	END
SUB1	START
LOOP	CALL	SUB2
	CPA	GR7,=0
	JZE	CAN
	LD	GR3,GR4
	LD	GR4,GR7
