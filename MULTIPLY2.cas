MULTIPLY	START
		LD	GR0,A
		LD	GR1,B
		CALL	MUL
		ST	GR2,ANS
		LD	GR0,C
		LD	GR1,D
		CALL	MUL
		ADDA	GR2,ANS
		ST	GR2,ANS
		RET
MUL		START
		LD	GR2,CONST0
		LD	GR3,CONST0
LOOP		CPA	GR3,GR1
		JZE	LABEL1
		ADDA	GR2,GR0
		ADDA	GR3,CONST1
		JUMP	LOOP
LABEL1		RET
A		DC	2
B		DC	3
C		DC	4
D		DC	5
ANS		DC	0
CONST0		DC	0
CONST1		DC	1
		END