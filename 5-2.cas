MAIN	START
	LD	GR3,=25
	CALL	SUB1
	ST	GR7,#0080
	LD	GR3,=31
	CALL	SUB1
	ST	GR7,#0081
	RET
SUB1	LD	GR7,-1
	LD	GR4,GR3
	SRA	GR4,1
LOOP	CPA	GR4,=1
	JZE	SOSU
	LD	GR5,GR3
	LD	GR6,GR4
	CALL	SUB2
	CPA	GR7,=0
	RET
	CPA	GR7,=1
	RET
	JUMP	LOOP
SOSU	LD	GR7,=1
	RET
NSOSU	LD	GR7,=0
	RET
SUB2	CPA	GR5,GR6
	JZE	CAN
	JMI	CANT
	SUBA	GR5,GR6
	JUMP	SUB2
CAN	JUMP	NSOSU
CANT	SUBA	GR4,=1
	JUMP	LOOP
	END
	